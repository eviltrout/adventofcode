{
  "env": {
    "node": true,
    "mocha": true,
  },
  ecmaVersion: 7,
  "globals":
    {"require":true,
    "requirejs":true,
    "hasModule":true,
    "Blob":true,
    "File":true},
  "rules": {
    "block-scoped-var": 2,
    "dot-notation": 0,
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "guard-for-in": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": 0,
    "no-debugger": 2,
    "no-empty": 0,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": 0,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new": 2,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-with": 2,
    "semi": 2,
    "strict": 0,
    "valid-typeof": 2,
    "wrap-iife": [
      2,
      "inside"
    ]
  },
  "parser": "babel-eslint"
}
